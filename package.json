		"dist"
	],
	"scripts": {
		"build": "rm -rf dist && tsc",
		"prepare": "npm run build",
		"test": "echo \"Error: no test specified\" && exit 1",
		"publish": "npm publish --access=public",
		"check": "biome check --write"
	},
