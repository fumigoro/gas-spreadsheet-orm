{
	"name": "@fumigoro/gas-spreadsheet-object-mapper",
	"version": "0.1.5",
	"description": "Typed object mapper for Spreadsheet on Google Apps Script",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.js"
		}
	},
