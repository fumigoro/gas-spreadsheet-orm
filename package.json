	},
	"bugs": {
		"url": "https://github.com/fumigoro/gas-spreadsheet-orm/issues",
		"email": "gas-spreadsheet-orm@fumigoro.com"
	},
	"homepage": "https://github.com/fumigoro/gas-spreadsheet-orm#readme",
	"keywords": [
		"google-apps-script",
		"spreadsheet",
		"object-mapper",
		"orm"
	],
	"dependencies": {
		"typescript": "^5.4.5"
	},
